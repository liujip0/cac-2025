{
  "name": "@cac-2025/monorepo",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --names app,api -c green,blue \"pnpm app:dev\" \"pnpm api:dev\"",
    "app:dev": "cd app && pnpm dev",
    "api:dev": "cd api && pnpm dev",
    "create-environment": "cp app/.env.local.example app/.env.local && cp api/.dev.vars.example api/.dev.vars",
    "create-environment-windows": "cd app && copy .env.local.example .env.local && cd ../api && copy .dev.vars.example .dev.vars",
    "migrations": "cd api && pnpm wrangler d1 migrations"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3"
  }
}
